<ul class="versions test-addon-versions">
  {{#each versionsWithMeta as |version|}}
    <li>
      {{#if version.isEmber}}
        <EmberVersionItem @version={{version.version}} />
      {{else}}
        <AddonVersionItem @version={{version.version}} />
      {{/if}}
    </li>
  {{/each}}
  {{#if thereAreHiddenVersions}}
    <a href="#" {{action "showAllVersions"}}>Show all versions</a>
  {{/if}}
</ul>
