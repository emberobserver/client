<h4>Dependencies</h4>
<DependencyTable @fetchData={{action "fetchDependencies" addonVersion.id}} @collapsed={{false}} @description={{concat "Version " addonVersion.version " of this addon depends on the following addons:"}} @noAddonsMessage={{concat "Version " addonVersion.version " of this addon does not depend on any other addons."}} @class="test-addon-dependencies" />
